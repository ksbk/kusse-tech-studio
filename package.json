{
  "name": "kusse-tech-studio",
  "version": "1.0.0",
  "description": "KusseTechStudio Portfolio Website",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:css && npm run build:js",
    "build:css": "sass app/static/src/scss/main.scss app/static/dist/css/styles.css --style compressed",
    "build:js": "webpack --mode production",
    "dev": "npm run dev:css & npm run dev:js",
    "dev:css": "sass app/static/src/scss/main.scss app/static/dist/css/styles.css --watch",
    "dev:js": "webpack --mode development --watch",
    "lint": "npm run lint:scss && npm run lint:js",
    "lint:scss": "stylelint 'app/static/src/scss/**/*.scss'",
    "lint:js": "eslint app/static/src/js/**/*.js",
    "lint:fix": "npm run lint:scss -- --fix && npm run lint:js -- --fix",
    "optimize:images": "imagemin 'app/static/src/img/**/*.{jpg,jpeg,png,svg}' --out-dir='app/static/dist/img'",
    "analyze": "webpack-bundle-analyzer app/static/dist/js/bundle.js",
    "test": "jest",
    "clean": "rimraf app/static/dist/*"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.8.1",
    "eslint": "^8.51.0",
    "eslint-config-standard": "^17.1.0",
    "imagemin": "^8.0.1",
    "imagemin-cli": "^7.0.0",
    "jest": "^29.7.0",
    "mini-css-extract-plugin": "^2.7.6",
    "rimraf": "^5.0.5",
    "sass": "^1.69.4",
    "stylelint": "^15.11.0",
    "stylelint-config-standard-scss": "^11.1.0",
    "webpack": "^5.89.0",
    "webpack-bundle-analyzer": "^4.9.1",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.31"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/username/kusse-tech-studio.git"
  },
  "keywords": [
    "portfolio",
    "python",
    "flask",
    "tailwind",
    "iceland"
  ],
  "author": "KusseTechStudio",
  "license": "MIT"
}
